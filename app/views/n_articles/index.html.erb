<p id="notice"><%= notice %></p>

<h1>Listing N Articles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @n_articles.each do |n_article| %>
      <tr>
        <td><%= n_article.title %></td>
        <td><%= n_article.body %></td>
        <td><%= link_to 'Show', n_article %></td>
        <td><%= link_to 'Edit', edit_n_article_path(n_article) %></td>
        <td><%= link_to 'Destroy', n_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New N article', new_n_article_path %>
