<p id="notice"><%= notice %></p>

<h1>Listing M Articles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @m_articles.each do |m_article| %>
      <tr>
        <td><%= m_article.title :title %></td>
        <td><%= link_to 'Show', m_article %></td>
        <td><%= link_to 'Edit', edit_m_article_path(m_article) %></td>
        <td><%= link_to 'Destroy', m_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New M article', new_m_article_path %>
